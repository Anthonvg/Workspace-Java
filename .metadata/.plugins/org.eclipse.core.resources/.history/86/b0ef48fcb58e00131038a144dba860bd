package fes.aragon;

public class Principal {

	public static void main(String[] args) { //Esto es el menu principal
		
		Computadora computadora1 = new Computadora();
		Computadora computadora2 = new Computadora(60000.00);
		
		Celular celular1 = new Celular();
		Celular celular2 = new Celular(100.00);
		
		Automovil automovil1 = new Automovil();
		Automovil automovil2 = new Automovil(4);
		
		
		//Impresion en pantalla
		System.out.println("-----Class Computadora----------");
		System.out.println(computadora1.costo);
		System.out.println(computadora2.costo);
		System.out.println(computadora1.getModelo());//Metodo get para atributos private
		System.out.println("-----Class Celular--------------");
		System.out.println(celular1.costo);
		System.out.println(celular2.costo);
		System.out.println(celular1.getCompa–ia());
		System.out.println("-----Class Automovil------------");
		System.out.println(automovil1.llantas);
		System.out.println(automovil2.llantas);
		System.out.println(automovil1.getCilindros());
		
		

	}

}
